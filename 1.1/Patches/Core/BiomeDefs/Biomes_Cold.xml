<?xml version="1.0" encoding="utf-8"?>
<Patch>

    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Core</li>
        </mods>
        <match Class="PatchOperationSequence">
            <success>Always</success>
            <operations>
				<li Class="PatchOperationRemove">
					<xpath>/Defs/BiomeDef[defName = "ColdBog"]/wildPlants/Plant_TreeWillow</xpath>
				</li>
				<li Class="PatchOperationRemove">
					<xpath>/Defs/BiomeDef[defName = "ColdBog"]/wildPlants/Plant_TreeCypress</xpath>
				</li>
				<li Class="PatchOperationRemove">
					<xpath>/Defs/BiomeDef[defName = "ColdBog"]/wildPlants/Plant_TreeMaple</xpath>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/BiomeDef[defName = "ColdBog"]/wildPlants</xpath>
					<value>
						<Plant_Grass>1.0</Plant_Grass>
						<RG-CB_Plant_ReedGrass>3.2</RG-CB_Plant_ReedGrass>
						<RG-CB_Dervish>0.6</RG-CB_Dervish>
						<Plant_TreePine>0.1</Plant_TreePine>
						<RG-CB_Plant_BogTreePine>0.6</RG-CB_Plant_BogTreePine>
						<RG-CB_Volopus>0.1</RG-CB_Volopus>
						<Plant_TreeMaple>0.1</Plant_TreeMaple>
					</value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/BiomeDef[defName = "ColdBog"]/wildPlants/Plant_Moss</xpath>
					<value>
						<Plant_Moss>3.0</Plant_Moss>
					</value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/BiomeDef[defName = "ColdBog"]/terrainPatchMakers/li/thresholds[@name="MossyTerrain"]/min</xpath>
					<value>
						<plantDensity>0.4</plantDensity>
					</value>
				</li>
            </operations>
        </match>
    </Operation>

</Patch>