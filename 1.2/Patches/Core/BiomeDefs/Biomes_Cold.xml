<?xml version="1.0" encoding="utf-8"?>
<Patch>

    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Core</li>
        </mods>
        <match Class="PatchOperationSequence">
            <success>Always</success>
            <operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/BiomeDef[defName = "ColdBog"]/wildPlants</xpath>
					<value>
						<RG-CBE_Plant_ReedGrass>3.0</RG-CBE_Plant_ReedGrass>
						<RG-CBE_Dervish>0.5</RG-CBE_Dervish>
						<RG-CBE_Plant_SpruceTreePine>0.6</RG-CBE_Plant_SpruceTreePine>
						<RG-CBE_Plant_KarimTree>0.6</RG-CBE_Plant_KarimTree>
						<RG-CBE_Volopus>0.05</RG-CBE_Volopus>
						<RG-CBE_SwampPod>0.15</RG-CBE_SwampPod>
					</value>
				</li>
            </operations>
        </match>
    </Operation>

</Patch>