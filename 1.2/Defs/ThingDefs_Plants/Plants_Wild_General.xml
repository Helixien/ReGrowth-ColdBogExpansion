<?xml version="1.0" encoding="utf-8"?>
<Defs>

    <ThingDef ParentName="PlantBase">
        <defName>RG-CB_Volopus</defName>
        <label>volopus</label>
        <description>A small brown mushroom with holes growing in clusters. It grows exclusively in cold bogs.</description>
        <graphicData>
            <texPath>Things/Plant/Volopus</texPath>
            <graphicClass>Graphic_Random</graphicClass>
        </graphicData>
        <statBases>
            <MaxHitPoints>120</MaxHitPoints>
            <Beauty>2</Beauty>
            <Nutrition>0.4</Nutrition>
        </statBases>
        <selectable>true</selectable>
        <neverMultiSelect>false</neverMultiSelect>
        <pathCost>5</pathCost>
        <ingestible />
        <plant>
            <fertilityMin>0.04</fertilityMin>
            <fertilitySensitivity>0.15</fertilitySensitivity>
            <growDays>20</growDays>
            <maxMeshCount>4</maxMeshCount>
            <dieIfLeafless>true</dieIfLeafless>
            <harvestYield>12</harvestYield>
            <harvestTag>Standard</harvestTag>
            <harvestedThingDef>RawFungus</harvestedThingDef>
            <neverBlightable>true</neverBlightable>
            <visualSizeRange>0.3~0.5</visualSizeRange>
            <topWindExposure>0.1</topWindExposure>
            <purpose>Food</purpose>
        </plant>
    </ThingDef>
	
    <ThingDef ParentName="PlantBase">
        <defName>RG-CB_Plant_ReedGrass</defName>
        <label>reed grass</label>
        <description>Wild reed grass. Slows down anyone moving over it, but looks beautiful in the wind.</description>
        <statBases>
            <MaxHitPoints>100</MaxHitPoints>
            <Flammability>1.3</Flammability>
            <Beauty>2</Beauty>
            <Nutrition>0.25</Nutrition>
        </statBases>
        <pathCost>16</pathCost>
        <graphicData>
            <texPath>Things/Plant/ReedGrass</texPath>
            <graphicClass>Graphic_Random</graphicClass>
        </graphicData>
        <hideAtSnowDepth>0.5</hideAtSnowDepth>
        <ingestible />
        <plant>
            <fertilitySensitivity>0.7</fertilitySensitivity>
            <growDays>4</growDays>
            <harvestWork>60</harvestWork>
            <maxMeshCount>9</maxMeshCount>
            <visualSizeRange>0.90~1.10</visualSizeRange>
            <topWindExposure>0.4</topWindExposure>
            <wildClusterRadius>4</wildClusterRadius>
            <wildClusterWeight>10</wildClusterWeight>
            <wildOrder>1</wildOrder>
        </plant>
    </ThingDef>

</Defs>